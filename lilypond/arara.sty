% the feature command
\newcommand{\feature}[1]{%
    \begin{tikzpicture}[baseline=-0.65ex]%
        \node[
            fill=araracolour,
            inner sep=1pt,
            minimum height=0.5cm,
            minimum width=0.8cm,
            rounded corners=3pt,
            font=\color{white}\sffamily\bfseries\tiny
        ] {#1};
    \end{tikzpicture}%
}

