% colour definitions
% > for this release, the logotype colour will
% > be dark spring green: #177245
\RequirePackage{xcolor}
\definecolor{araracolour}{rgb}{0.09, 0.45, 0.27}
\definecolor{keywordcolour}{rgb}{.07,.61,.75}
\definecolor{commentcolour}{rgb}{0.07,0.75,0.42}

\RequirePackage{enumitem}
\RequirePackage{upquote}
\RequirePackage{hologo}
\RequirePackage{multicol}

% font stuff
\RequirePackage{cantarell}
\RequirePackage[notextcomp]{kpfonts}
\RequirePackage{inconsolata}

% ornament stuff
\RequirePackage{fourier-orns}

% TikZ stuff
\RequirePackage{tikz}
\usetikzlibrary{trees}
\usetikzlibrary{calc}

% BibLaTeX stuff
\RequirePackage[
    backend=biber,
    backref=true,
    hyperref=auto,
    refsection=chapter,
    citereset=chapter
    ]{biblatex}

% listings stuff
\RequirePackage{listings}

% used for numbered code listings
\lstnewenvironment{numberedcodelisting}{%
    \lstset{%
        basicstyle=\small\ttfamily,
        columns=flexible,
        showspaces=false,
        numbers=left,
        xleftmargin=1.5em,
        breaklines=true,
        numberstyle=\color{araracolour!50}\ttfamily,
        breakatwhitespace=true,
        showstringspaces=false,
        basewidth={0.5em,0.5em}
    }%
}{}

% used for unnumbered code listings
\lstnewenvironment{codelisting}{%
    \lstset{%
        basicstyle=\small\ttfamily,
        columns=flexible,
        showspaces=false,
        breaklines=true,
        numberstyle=\color{araracolour!50}\ttfamily,
        breakatwhitespace=true,
        showstringspaces=false,
        basewidth={0.5em,0.5em}
    }%
}{}

% used for LaTeX code listings
\lstnewenvironment{latexcodelisting}{%
    \lstset{%
        language=[LaTeX]TeX,
        basicstyle=\small\ttfamily,
        columns=flexible,
        showspaces=false,
        numbers=left,
        xleftmargin=1.5em,
        breaklines=true,
        numberstyle=\color{araracolour!50}\ttfamily,
        commentstyle=\color{araracolour},
        keywordstyle=\color{keywordcolour},
        texcsstyle=*\color{keywordcolour},
        breakatwhitespace=true,
        showstringspaces=false,
        basewidth={0.5em,0.5em},
        escapeinside={(*@}{@*)}
    }%
}{}

% short code listing
\lstMakeShortInline[basicstyle=\ttfamily]{|}

% create a new code environment
\newcommand{\codename}{Code}
\newcommand{\listcodename}{List of Codes}
\newlistof{listofcodes}{cod}{\listcodename}
\newfloat{code}{cod}{\codename}
\newlistentry{code}{cod}{0}

% framing stuff
\RequirePackage[framemethod=TikZ]{mdframed}

% used for remarks
\newmdenv[
    nobreak,
    hidealllines=true,
    middlelinewidth=.8pt,
    frametitlefont=\normalfont\sffamily\small\bfseries\color{araracolour},
    font=\small\em,
    linecolor=araracolour,
    leftmargin=.3cm,
    rightmargin=.3cm,
    innerleftmargin=2cm,
    skipabove=\topsep,
    skipbelow=\topsep,
    singleextra={%
        \path let \p1=(P), \p2=(O) in ($(\x2,0)+0.5*(2,\y1)$) node[scale=0.5,overlay] {\rubberduck};
        \draw[line width=.8pt,araracolour,] ($(O)+(.2cm,0)$)  -- (O) -- (O|-P) -- ($(O|-P)+(.2cm,0)$); 
        \draw[line width=.8pt,araracolour,] ($(P|-O)-(.2cm,0)$) -- (P|-O) -- (P) -- ($(P)-(.2cm,0)$);
    }%
]{remark}

% used for listing frames
\newmdenv[
    nobreak,
    middlelinewidth=.8pt,
    roundcorner=8pt,
    linecolor=araracolour,
    innertopmargin=0pt,
    innerbottommargin=0pt,
    skipabove=0pt,
    skipbelow=0pt,
    backgroundcolor=araracolour!4
]{listingframe}

